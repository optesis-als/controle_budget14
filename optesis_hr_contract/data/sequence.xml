<?xml version="1.0" encoding="utf-8"?>
<odoo>
   
     <data>
        <!-- Sequences for sale.order -->
        <record id="seq_contrat" model="ir.sequence">
            <field name="name">Séquence contrat</field>
            <field name="code">hr.contract</field>
            <field name="prefix">CT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
         
          <record id="seq_matricule" model="ir.sequence">
            <field name="name">Séquence matricule</field>
            <field name="code">hr.employee.seq</field>
            <field name="prefix">MAT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>